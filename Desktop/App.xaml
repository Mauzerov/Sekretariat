<Application x:Class="Desktop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Desktop"
             xmlns:viewModel="clr-namespace:Desktop.View.Table.Viewer"
             xmlns:view="clr-namespace:Desktop.View.Table.Viewer"
             xmlns:viewer="clr-namespace:Desktop.View.Viewer"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         <ResourceDictionary>
             <ResourceDictionary.MergedDictionaries>
                 <ResourceDictionary Source="Styles/Table/TabSelector.xaml"></ResourceDictionary>
             </ResourceDictionary.MergedDictionaries>
             <DataTemplate DataType="{x:Type viewer:TableViewer}">
                <view:StudentTable/>
             </DataTemplate>
         </ResourceDictionary>
    </Application.Resources>
</Application>
